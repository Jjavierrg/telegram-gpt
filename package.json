{
  "name": "telegram-gpt",
  "version": "1.0.0",
  "description": "Telegram bot using GPT",
  "main": "build/index.js",
  "files": [
    "build",
    "!build/**/*.test.*"
  ],
  "scripts": {
    "prepare": "npm run build",
    "prestart": "npm run build",
    "start": "node build/index.js",
    "build": "npm run lint && npm test && tsc",
    "lint": "eslint . --ext .ts",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest --verbose",
    "test:watch": "npm t -- --watchAll",
    "test:coverage": "npm t -- --coverage",
    "docs:generate": "rm -rf docs && typedoc src/ --out docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jjavierrg/telegram-gpt.git"
  },
  "keywords": [],
  "author": "Ulises Santana <ulisesantana@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Jjavierrg/telegram-gpt/issues"
  },
  "homepage": "https://github.com/Jjavierrg/telegram-gpt#readme",
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "@types/node": "^18.15.12",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.38.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.1",
    "ts-jest": "^29.1.0",
    "typedoc": "^0.24.4",
    "typedoc-plugin-markdown": "^3.15.1",
    "typescript": "^5.0.4"
  }
}